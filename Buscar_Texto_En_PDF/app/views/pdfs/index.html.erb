<h1>Listado de PDF</h1>

<table>
  <tr>
    <th>Nombre archivo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pdfs.each do |pdf| %>
  <tr>
    <td><%= pdf.nombreArchivo %></td>
    <td><%= link_to 'Show', pdf %></td>
    <td><%= link_to 'Edit', edit_pdf_path(pdf) %></td>
    <td><%= link_to 'Destroy', pdf, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pdf', new_pdf_path %>
